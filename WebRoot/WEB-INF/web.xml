<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>system_2</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.cr.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NodeAddServlet</servlet-name>
    <servlet-class>com.cr.servlet.NodeAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NodeSearchServlet</servlet-name>
    <servlet-class>com.cr.servlet.NodeSearchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NodeDelServlet</servlet-name>
    <servlet-class>com.cr.servlet.NodeDelServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RoleAddServlet</servlet-name>
    <servlet-class>com.cr.servlet.RoleAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RoleSearchServlet</servlet-name>
    <servlet-class>com.cr.servlet.RoleSearchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserAddServlet</servlet-name>
    <servlet-class>com.cr.servlet.UserAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserSearchServlet</servlet-name>
    <servlet-class>com.cr.servlet.UserSearchServlet</servlet-class>
  </servlet>








  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/loginServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NodeAddServlet</servlet-name>
    <url-pattern>/nodeAddServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NodeSearchServlet</servlet-name>
    <url-pattern>/nodeSearchServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NodeDelServlet</servlet-name>
    <url-pattern>/nodeDelServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RoleAddServlet</servlet-name>
    <url-pattern>/roleAddServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RoleSearchServlet</servlet-name>
    <url-pattern>/roleSearchServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserAddServlet</servlet-name>
    <url-pattern>/userAddServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserSearchServlet</servlet-name>
    <url-pattern>/userSearchServlet.do</url-pattern>
  </servlet-mapping>
  
  
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>Public/login.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>